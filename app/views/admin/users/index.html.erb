<h1>Admin user index </h1>

<% @users.each do |user| %>
	<dl>
		<dt style="font-weight:bold"><%="Name: "%><%=user.name %> </dt>
		<dd><%="Email: "%><%=user.email %> </dd>
		<dd><%="Role: "%><%=user.role %> </dd>
	</dl>
	<%= link_to('Edit role',edit_admin_user_path(user), class: "actions btn btn-primary", method: :get) %>
<% end %>